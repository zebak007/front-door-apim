{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.1",
  "parameters": {
    "location": {
      "value": "West Europe"
    },
    "workspaceName": {
      "value": "cbezFrontDoorApimDemoLogAnalytics"
    },
    "workspaceSku": {
      "value": "Standard"
    },
    "virtualNetwork": {
      "value": {
				"name": "cbezFrontDoorApimDemoVnet",
				"addressPrefixes": ["10.0.0.0/16"],
				"subnets": [
          {
						"name": "cbezApiManagementSubnet",
            "addressPrefix": "10.0.0.0/24",
            "nsg": "cbezFrontDoorApimDemoNsg"
					}
				]
			}
    },
    "appInsightsName": {
      "value": "cbezFrontDoorApimDemoAppInsights"
    },
    "apimServiceName": {
      "value": "cbezFrontDoorApimDemoApim"
    },
    "apimPublisherEmail": {
      "value": "root@lab.bezencon.net"
    },
    "apimPublisherName": {
      "value": "Bezencon.net"
    },
    "apimSku": {
      "value": "Developer"
    },
    "apimCapacity": {
      "value": 1
    },
    "frontDoorName": {
      "value": "cbezFrontDoorApimDemoAfd"
    },
    "frontDoorFrontendEndpoint": {
      "value": {
        "name": "MainFrontendEndpoint",
        "sessionAffinityEnabledState": "Disabled",
        "sessionAffinityTtlSeconds": 0
      }
    },
    "frontDoorBackendPool": {
      "value": {
        "name": "ApimBackendPool",
        "loadBalancerName": "ApimLoadBalancer"
      }
    },
    "frontDoorRoutingRule": {
      "value": {
        "name": "ApimRoutingRule",
        "acceptedProtocols": [
          "Http",
          "Https"
        ],
        "patternsToMatch": [
          "/*"
        ],
        "customForwardingPath": "/",
        "forwardingProtocol": "MatchRequest",
        "cacheConfiguration": {
          "queryParameterStripDirective": "StripNone",
          "dynamicCompression": "Enabled"
        }
      }
    },
    "frontDoorHealthProbeSettings": {
      "value": {
        "name": "ApimHealthProbeSettings",
        "intervalInSeconds": 30,
        "path": "/status-0123456789abcdef",
        "protocol": "Https"
      }
    },
    "deployWaf": {
      "value": true
    },
    "wafPolicyName": {
      "value": "FrontDoorApimDemoWaf"
    },
    "wafMode": {
      "value": "Detection"
    },
    "allowTrafficOnlyFromFrontDoor": {
      "value": false
    }
  }
}
